      "source": [
        "hidden_units = 256\n",
        "encoder = Encoder(len(vocab)+1, embedding_dim, hidden_units, embedding_matrix) # +2 on due to start and end tokens\n",
        "decoder = Decoder(len(vocab)+1, embedding_dim, hidden_units, embedding_matrix)"
